@model IEnumerable<Product>

@{ 
    ViewBag.Title = "List of products";
}

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach( var item in Model )
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
            </tr>
            }
            
    </tbody>
</table>
